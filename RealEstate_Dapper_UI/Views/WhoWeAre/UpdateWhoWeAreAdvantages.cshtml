@model UpdateWhoWeAreAdvantagesDto
@{
    ViewData["Title"] = "UpdateWhoWeAreAdvantages";
    Layout = "~/Views/Admin/Index.cshtml";
}

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Yeni Avantaj Ekleme </h6>
                <form method="post">
                    <input type="hidden" asp-for="AdvantagesID" />
                    <label>Avantaj Adı</label>
                    <input type="text" asp-for="AdvantagesName" class="form-control" />
                    <br />
                    <button class="btn btn-success">Avantajı Güncelle</button>
                </form>
            </div>
        </div>
    </div>
</div>

